// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Teachers {
  id               String @id @default(uuid())
  name             String
  register         Int 
  cpf              Int 
  turn             String
  createdAt        DateTime @default(now())

  @@map("teachers")
}

model Students {
  id             String @id @default(uuid())
  name           String
  objective      String
  rg             Int 
  cpf            Int 
  address        String
  contact        Int
  email          String
  createdAt      DateTime @default(now())

  @@map("students")
}

model Sets {
  id           Int @id @default(autoincrement())
  type         String
  createdAt    DateTime @default(now())

  @@map("sets")
}

model Exercises {
  id             Int @id @default(autoincrement())
  name           String
  reps           Int
  createdAt      DateTime @default(now())

  @@map("exercises")
}
