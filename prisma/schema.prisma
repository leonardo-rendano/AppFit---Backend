// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Teachers {
  id               Int @id @default(autoincrement())
  teacher_name     String
  teacher_register Int @unique
  teacher_cpf      Int @unique
  teacher_turn     String
  createdAt        DateTime @default(now())

  @@map("teachers")
}

model Students {
  id             Int @id @default(autoincrement())
  name           String
  objective      String
  rg             Int 
  cpf            Int 
  address        String
  contact        Int
  email          String
  createdAt      DateTime @default(now())

  @@map("students")
}

model Sets {
  id           Int @id @default(autoincrement())
  set_quantity Int
  set_type     String
  createdAt    DateTime @default(now())

  @@map("sets")
}

model Exercises {
  id             Int @id @default(autoincrement())
  exercise_name  String
  exercise_reps  Int
  exercise_type  String
  createdAt      DateTime @default(now())

  @@map("exercises")
}
